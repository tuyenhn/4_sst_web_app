{
  "name": "4_sst_web_app",
  "version": "0.0.1",
  "description": "web app development for RMIT's number 4 ",
  "main": "./js/index.js",
  "scripts": {
    "build:css": "npx postcss css/template.css -o prod/css/template.css",
    "build:index": "npx uglifyjs js/index.js -o prod/js/index.js",
    "build:service-worker": "npx uglifyjs js/service-worker.js -o prod/js/service-worker.js",
    "build:js": "npm run build:index && npm run build:service-worker",
    "watch": "watch \"npm run build:css\" ./css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuyenhn/4_sst_web_app.git"
  },
  "author": "Tuyen Huynh",
  "bugs": {
    "url": "https://github.com/tuyenhn/4_sst_web_app/issues"
  },
  "homepage": "https://github.com/tuyenhn/4_sst_web_app#readme",
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "tailwindcss": "^2.0.2",
    "uglify-js": "^3.12.3",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "jquery": "^3.5.1",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0"
  }
}